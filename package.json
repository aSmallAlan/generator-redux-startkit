{
    "name": "generator-redux-startkit",
    "version": "0.0.0",
    "description": "A generator for multiple-page application with react and redux.",
    "homepage": "https://github.com/feflow/generator-redux-startkit",
    "author": {
        "name": "leoytliu",
        "email": "liuyitao811@hotmail.com",
        "url": "https://leoeatle.github.io/techBlog/"
    },
    "files": [
        "generators"
    ],
    "main": "generators/index.js",
    "keywords": [
        "feflow",
        "react",
        "redux",
        "muiltiple-page",
        "yeoman-generator"
    ],
    "devDependencies": {
        "commitizen": "^3.0.5",
        "coveralls": "^3.0.2",
        "eslint": "^5.9.0",
        "eslint-config-prettier": "^3.3.0",
        "eslint-config-xo": "^0.25.0",
        "eslint-plugin-prettier": "^3.0.0",
        "glob-all": "^3.1.0",
        "husky": "^1.2.0",
        "jest": "^23.5.0",
        "lint-staged": "^8.1.0",
        "prettier": "^1.15.2",
        "yeoman-assert": "^3.1.0",
        "yeoman-test": "^1.7.0"
    },
    "engines": {
        "npm": ">= 4.0.0"
    },
    "dependencies": {
        "yeoman-generator": "^2.0.1",
        "chalk": "^2.1.0",
        "yosay": "^2.0.1"
    },
    "jest": {
        "testEnvironment": "node"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ],
        "*.json": [
            "prettier --write",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": [
            "xo",
            "prettier"
        ],
        "env": {
            "jest": true,
            "node": true
        },
        "rules": {
            "prettier/prettier": "error"
        },
        "plugins": [
            "prettier"
        ]
    },
    "scripts": {
        "pretest": "eslint . --fix",
        "test": "jest",
        "commit": "git-cz"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "repository": "https://github.com/feflow/generator-redux-startkit.git",
    "license": "MIT"
}
