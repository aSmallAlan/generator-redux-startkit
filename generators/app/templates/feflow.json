{
    "builderType": "builder-webpack4",
    "builderOptions": {
        "moduleName": "<%= moduleName %>",
        "bizName": "<%= bizName %>",
        "minifyHTML": true,
        "minifyCSS": true,
        "minifyJS": true,
        "inlineCSS": true,
        "usePx2rem": false,
        "remUnit": 102,
        "remPrecision": 6,
        "inject": true,
        "port": 8001,
        "externals": [
            {
                "module": "react",
                "entry": "https://unpkg.com/react@16/umd/react.development.js",
                "global": "React"
            },
            {
                "module": "react-dom",
                "entry": "https://unpkg.com/react-dom@16/umd/react-dom.development.js",
                "global": "ReactDOM"
            }
        ]
    }
}
